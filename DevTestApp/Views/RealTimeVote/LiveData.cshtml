@model IEnumerable<DevTestApp.DevTest>

@{
    ViewBag.Title = "Index";
}

<h2>Live Data</h2>

<table class="table" style="color:green">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CampaignName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Clicks__int_NULL___Conversions)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Impressions)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AffiliateName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CampaignName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Clicks__int_NULL___Conversions)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Impressions)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AffiliateName)
            </td>
        </tr>
    }
</table>

